// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider   = "prisma-client"
  output     = "./generated"
  engineType = "client"
}

datasource db {
  provider = "sqlite"
}

/// Событие афиши
model Event {
  id        Int      @id @default(autoincrement())
  title     String
  date      DateTime
  address   String
  link1     String?
  link2     String?
  createdAt DateTime @default(now())
}
